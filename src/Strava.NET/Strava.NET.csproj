<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\Strava.NET.snk</AssemblyOriginatorKeyFile>
    <Authors>Srini Vasudeavan</Authors>
    <PackageProjectUrl>https://github.com/srini85/Strava.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/srini85/Strava.NET</RepositoryUrl>
    <PackageTags>strava</PackageTags>
    <Description>Auto Generated Strava .NET library from the Strava V3 API.
Unlike many other auto generated Strava API wrapper libraries on Nuget, this package is signed</Description>
    <PackageLicenseExpression>https://github.com/srini85/Strava.NET/blob/master/LICENSE.txt</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="RestSharp" Version="106.6.10" />
  </ItemGroup>

</Project>
